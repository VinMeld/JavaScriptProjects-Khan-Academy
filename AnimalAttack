var bodyX = 200;
var bodyY = 220;
var bodyW = 118;
var bodyH = bodyW/2;
var speed = 1;
draw = function() {
    background(207, 254, 255);
    fill(240, 209, 36);
    bodyX+=speed;
    
    if(bodyX === 300){
            speed -= 1;
    }
    if(bodyX === 100){
        speed = 1;
    
    }
    ellipse(bodyX, bodyY, bodyW, 106); // body?
    ellipse(bodyX, bodyY-70, bodyH, 47); // face?
};
